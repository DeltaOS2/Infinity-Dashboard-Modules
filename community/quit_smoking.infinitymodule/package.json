{
  "name": "Quit Smoking",
  "description": "Displays useful stats since quitting smoking",
  "author": "Jessica Ramirez",
  "website": "https://infinitydashboard.com/",
  "category": "health",
  "bundleID": "com.jessicaramirez.quitsmoking",
  "version": "1.0.0",
  "minimumRefreshInterval": 60,
  "user_options": [
    {
      "title": "Display",
      "name": "showAs",
      "type": "string",
      "options": [
        {"title": "Amount Saved", "name": "saved"},
        {"title": "Days Smoke Free", "name": "date"},
        {"title": "Cigarettes not smoked", "name": "cigs"},
      ],
      "default": "saved",
      "required": true,
    },

    {
      "title": "Date quit smoking",
      "name": "date",
      "type": "date",
      "required": true,
      "options": {
        "allowsDate": true,
        "allowsTime": false
      }
    },
    {
      "title": "Cigarettes per day",
      "name": "perDay",
      "type": "number",
      "required": true,
      "options": {
        "min": 1,
      }
    },
    {
      "title": "Cigarettes per pack",
      "name": "perPack",
      "type": "number",
      "required": true,
      "options": {
        "min": 1
      }
    },
    {
      "title": "Price per pack",
      "name": "pricePerPack",
      "type": "number",
      "required": true,
      "options": {
        "min": 1
      }
    },
